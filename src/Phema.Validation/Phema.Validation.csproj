<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>8</LangVersion>
		<RootNamespace>Phema.Validation</RootNamespace>
		<Version>3.0.7</Version>
		<NullableReferenceTypes>true</NullableReferenceTypes>
		<Authors>sergeyshaykhullin</Authors>
		<Description>C# strongly typed validation library for .NET</Description>
		<PackageTags>.NETCore;ASP.NETCore;Validation</PackageTags>
		<PackageProjectUrl>https://github.com/sergeyshaykhullin/Phema.Validation</PackageProjectUrl>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://github.com/sergeyshaykhullin/Phema.Validation</RepositoryUrl>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.0.0-preview7.19362.4" />
	  <PackageReference Include="Microsoft.Extensions.Options" Version="3.0.0-preview7.19362.4" />
	</ItemGroup>

</Project>
